<div class="account">
<h2>新規登録</h2>
<div class="account_forms">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <div class="field">
      <%= f.label :"プロフィール画像" %><br />
      <%= f.file_field :image %>
    </div>
    <div class="field">
      <%= f.label :"ユーザー名" %><br />
      <%= f.text_field :username, class: "form-control", class: "form" %>
    </div>
    <div class="field">
      <%= f.label :"メールアドレス" %><br />
      <%= f.email_field :email,  autocomplete: "email", class: "form" %>
    </div>
    <div class="field">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "new-password", class: "form" %>
    </div>

    <div class="field">
      <%= f.label :確認パスワード %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form" %>
    </div>

    <div class="actions">
      <%= f.submit "STERT" %>
    </div>
  <% end %>
</div>
<%= link_to "ログインはこちら", new_user_session_path, class: "login" %>
</div>